{
  "name": "cachify-wrapper",
  "version": "5.0.0",
  "description": "Wraps a function with a caching layer",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.js",
  "keywords": [
    "cache",
    "typescript",
    "ts"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.6",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "prepublishOnly": "npm run test",
    "prepack": "npm install --only=production && npm run js:build",
    "js:build": "tsc",
    "js:delete": "rm -rf ./build",
    "js:build:clear": "npm run js:delete && npm run js:build",
    "js:build:watch": "tsc --watch",
    "docs:build": "typedoc source/index.ts",
    "docs:delete": "rm -rf ./docs",
    "docs:build:clear": "npm run docs:delete && npm run docs:build",
    "all:build": "npm run js:build:clear && npm run docs:build:clear",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "preversion": "npm run test",
    "test": "mocha -r ts-node/register source/**/*test.ts",
    "test:watch": "mocha --watch -r ts-node/register source/**/*test.ts"
  },
  "optionalDependencies": {
    "@types/node": "^12.12.6",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-chepugov/cachify-wrapper.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a-chepugov/cachify-wrapper/issues"
  },
  "homepage": "https://github.com/a-chepugov/cachify-wrapper#readme"
}
